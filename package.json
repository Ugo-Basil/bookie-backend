    "typeorm": "^0.3.12"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/node": "^18.13.0",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
